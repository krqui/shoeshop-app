{
  "expo": {
    "name": "shoe-shop",
    "slug": "shoeshop-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/zapas.webp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets//zapas.webp",
      "resizeMode": "contain",
      "backgroundColor": "#2eb82e"
    },
    "updates":{
      "fallbackToCacheTimeout":0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/zapas.webp",
        "backgroundColor": "#2eb82e"
      }
    },
    "web": {
      "favicon": "./assets/zapas.webp"
    }
  }
}
